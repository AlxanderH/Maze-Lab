package mazepack;
//This class was made so that there could be a key object on the map.
public class Key {
	
	private Map map;
	private Location loc;
	
	//This constructor will construct the key object on the map.
  public Key(Map m, Location l) {
		map = m;
		loc = l;
	}
	//A quick return method to return map.
	public Map getMap() {return map;}
  //A quick return method to return location.
	public Location getLoc() {return loc;}
  //A quick return method to set map.
	public void setMap(Map m) {map = m;}
  //A quick return method to set location.
	public void setLoc(Location l) {loc = l;}
}
